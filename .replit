modules = ["python-3.11", "postgresql-16"]

[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["alsa-lib", "at-spi2-atk", "atk", "cups", "dbus", "espeak-ng", "expat", "file", "freetype", "gitFull", "glib", "lcms2", "libdrm", "libimagequant", "libjpeg", "libtiff", "libwebp", "libxcrypt", "libxkbcommon", "libyaml", "mesa", "nspr", "nss", "openjpeg", "pango", "playwright-driver", "tcl", "tk", "xorg.libX11", "xorg.libXcomposite", "xorg.libXdamage", "xorg.libXext", "xorg.libXfixes", "xorg.libXrandr", "xorg.libxcb", "xorg.libxshmfence", "zlib"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Manus Agent"

[[workflows.workflow]]
name = "Manus Agent"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd /home/runner/workspace && python3 web/server.py"
waitForPort = 5000

[[ports]]
localPort = 5000
externalPort = 80
