modules = ["python-3.11"]

[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["alsa-lib", "at-spi2-atk", "atk", "cups", "dbus", "expat", "gitFull", "glib", "libdrm", "libxkbcommon", "libyaml", "mesa", "nspr", "nss", "pango", "playwright-driver", "xorg.libX11", "xorg.libXcomposite", "xorg.libXdamage", "xorg.libXext", "xorg.libXfixes", "xorg.libXrandr", "xorg.libxcb", "xorg.libxshmfence"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Manus Agent"

[[workflows.workflow]]
name = "Manus Agent"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd /home/runner/workspace && python3 -m agent_core.main"

[workflows.workflow.metadata]
outputType = "console"
