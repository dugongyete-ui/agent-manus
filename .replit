modules = ["python-3.11"]

[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["gitFull", "libyaml", "playwright-driver", "nspr", "nss", "atk", "cups", "at-spi2-atk", "xorg.libXcomposite", "xorg.libXdamage", "xorg.libXrandr", "mesa", "pango", "alsa-lib", "xorg.libxcb", "xorg.libX11", "xorg.libXext", "xorg.libXfixes", "expat", "glib", "dbus", "libdrm", "xorg.libxshmfence", "libxkbcommon"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Manus Agent"

[[workflows.workflow]]
name = "Manus Agent"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd /home/runner/workspace && python3 -m agent_core.main"

[workflows.workflow.metadata]
outputType = "console"
